{
  "routes": [
    {
      "route": "/authenticated/*",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "userDetailsClaim": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/{{ $secret.TENANT_ID }}",
          "clientIdSettingName": "{{ $secret.AZURE_CLIENT_ID }}",
          "clientSecretSettingName": "{{ $secret.AZURE_CLIENT_SECRET }}"
        }
      }
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}
